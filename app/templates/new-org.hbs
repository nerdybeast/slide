{{#link-to "orgs"}}Back{{/link-to}}

<div>
	New org template
</div>

<form>
	<div class="form-group form-group-sm">
		<label for="name">Name</label>
		{{input id="name" class="form-control" value=name}}
	</div>
	<div class="form-group">
		<label for="orgType">Org Type</label>
		{{#power-select id="orgType" options=model.orgTypes onchange=(action "orgTypeSelection") as |orgType|}}
			{{orgType}}
		{{/power-select}}
	</div>
	<div class="form-group form-group-sm">
		<label for="username">Username</label>
		{{input id="username" class="form-control" value=username}}
	</div>
	<div class="form-group form-group-sm">
		<label for="password">Password</label>
		{{input id="password" class="form-control" type="password" value=password}}
	</div>
	<button {{action "addNewOrg"}} class="btn btn-default">Submit</button>
</form>

{{outlet}}
